<?xml version="1.0"?>
<robot name="first_shape">

  <!-- Materials -->
  <material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>
  
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>

  <!-- Base -->
  <link name="base_link">
    <visual>
      <geometry>
        <cylinder radius="0.2" length="0.6"/>
      </geometry>
      <material name="blue"/>
    </visual>
  </link>

  <!-- Right Arm -->
  <link name="right_link">
    <visual>
      <geometry>
        <box size="0.6 0.1 0.2"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>

  <joint name="base_to_right_link" type="fixed">
    <parent link="base_link"/>
    <child link="right_link"/>
    <origin xyz="0.0 -0.22 0.25"/>
  </joint>

  <link name="right_base">
    <visual>
      <geometry>
        <box size="0.4 0.1 0.1"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>

  <joint name="right_base_joint" type="fixed">
    <parent link="right_link"/>
    <child link="right_base"/>
    <origin xyz="0.0 0.0 -0.6"/>
  </joint>

  <link name="right_front_wheel">
    <visual>
      <geometry>
        <cylinder radius="0.035" length="0.1"/>
      </geometry>
      <origin rpy="1.57075 0 0"/>
      <material name="black"/>
    </visual>
  </link>

  <joint name="right_front_wheel_joint" type="fixed">
    <parent link="right_base"/>
    <child link="right_front_wheel"/>
    <origin xyz="0.1333 0 -0.085"/>
  </joint>

  <link name="right_back_wheel">
    <visual>
      <geometry>
        <cylinder radius="0.035" length="0.1"/>
      </geometry>
      <origin rpy="1.57075 0 0"/>
      <material name="black"/>
    </visual>
  </link>

  <joint name="right_back_wheel_joint" type="fixed">
    <parent link="right_base"/>
    <child link="right_back_wheel"/>
    <origin xyz="-0.1333 0 -0.085"/>
  </joint>

  <!-- Left Arm -->
  <link name="left_link">
    <visual>
      <geometry>
        <box size="0.6 0.1 0.2"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>

  <joint name="base_to_left_link" type="fixed">
    <parent link="base_link"/>
    <child link="left_link"/>
    <origin xyz="0.0 0.22 0.25"/>
  </joint>

  <link name="left_base">
    <visual>
      <geometry>
        <box size="0.4 0.1 0.1"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>

  <joint name="left_base_joint" type="fixed">
    <parent link="left_link"/>
    <child link="left_base"/>
    <origin xyz="0.0 0.0 -0.6"/>
  </joint>

  <link name="left_front_wheel">
    <visual>
      <geometry>
        <cylinder radius="0.035" length="0.1"/>
      </geometry>
      <origin rpy="1.57075 0 0"/>
      <material name="black"/>
    </visual>
  </link>

  <joint name="left_front_wheel_joint" type="fixed">
    <parent link="left_base"/>
    <child link="left_front_wheel"/>
    <origin xyz="0.1333 0 -0.085"/>
  </joint>

  <link name="left_back_wheel">
    <visual>
      <geometry>
        <cylinder radius="0.035" length="0.1"/>
      </geometry>
      <origin rpy="1.57075 0 0"/>
      <material name="black"/>
    </visual>
  </link>

  <joint name="left_back_wheel_joint" type="fixed">
    <parent link="left_base"/>
    <child link="left_back_wheel"/>
    <origin xyz="-0.1333 0 -0.085"/>
  </joint>

  <!-- Gripper -->
  <link name="gripper_pole">
    <visual>
      <geometry>
        <cylinder radius="0.01" length="0.2"/>
      </geometry>
      <origin xyz="0.1 0.0 0.0" rpy="0 1.57057 0"/>
    </visual>
  </link>

  <joint name="gripper_extention" type="fixed">
    <parent link="base_link"/>
    <child link="gripper_pole"/>
    <origin xyz="0.19 0.0 0.2"/>
  </joint>

  <link name="gripper_left">
    <visual>
      <geometry>
        <box size="0.05 0.02 0.1"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>

  <joint name="left_gripper_joint" type="fixed">
    <parent link="gripper_pole"/>
    <child link="gripper_left"/>
    <origin xyz="0.2 0.01 0"/>
  </joint>

  <link name="left_tip">
    <visual>
      <geometry>
        <box size="0.05 0.02 0.1"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>

  <joint name="left_tip_joint" type="fixed">
    <parent link="gripper_left"/>
    <child link="left_tip"/>
    <origin xyz="0.05 0 0"/>
  </joint>

  <link name="right_gripper">
    <visual>
      <geometry>
        <box size="0.05 0.02 0.1"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>

  <joint name="right_gripper_joint" type="fixed">
    <parent link="gripper_pole"/>
    <child link="right_gripper"/>
    <origin xyz="0.2 -0.01 0"/>
  </joint>

  <!-- Head -->
  <link name="head">
    <visual>
      <geometry>
        <sphere radius="0.2"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>

  <joint name="head_swivel" type="fixed">
    <parent link="base_link"/>
    <child link="head"/>
    <origin xyz="0 0 0.3"/>
  </joint>

  <!-- Box on Head -->
  <link name="box">
    <visual>
      <geometry>
        <box size="0.08 0.08 0.08"/>
      </geometry>
      <material name="blue"/>
    </visual>
  </link>

  <joint name="tobox" type="fixed">
    <parent link="head"/>
    <child link="box"/>
    <origin xyz="0.1814 0 0.1414"/>
  </joint>

</robot>